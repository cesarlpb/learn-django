{
  "apps": [
    {
      "name": "django",
      "script": "manage.py",
      "args": ["runserver", "0.0.0.0:8000"],
      "interpreter": "python3",
      "instances": "1",
      "autorestart": true,
      "watch": false,
      "max_memory_restart": "1G",
      "env": {
        "DJANGO_SETTINGS_MODULE": "proyecto.settings"
      }
    }
  ]
}
